package com.example.spring.repository;
import java.util.List;
import java.util.Optional;
import com.example.spring.entity.Order1;
public interface OrderRepository {
	Order1 insertOrder(Order1 order);

	  List<Order1> listAllOrders();
	  Optional<Order1> selectOrderById(Long id);
	  Long deleteOrderById(Long id);
	  Long updateOrderById(Long id,Order1 order);
}
